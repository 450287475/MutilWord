<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wangdao.mutilword">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.RetrievePasswordActivity" />
        <activity android:name=".activity.ReadActivity" />
        <activity android:name=".activity.ChooseWordTypeActivity"></activity>
        <activity android:name=".activity.ReciteWordActivity"></activity>
        <activity android:name=".activity.ShowArticleDetailActivity" />

        <activity android:name=".activity.PersonalCenterActivity">

        </activity>
    </application>

</manifest>